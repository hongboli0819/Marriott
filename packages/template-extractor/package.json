{
  "name": "@internal/template-extractor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "模版提取器 - 从图片中提取可编辑模版",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build:app": "vite build",
    "preview": "vite preview",
    "build:lib": "tsup src/core/index.ts --dts --format esm,cjs --sourcemap --clean --external react,react-dom,react-router-dom",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "fabric": "^6.6.2",
    "lucide-react": "^0.462.0",
    "opentype.js": "^1.3.4",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3",
    "vite": "^5.4.19"
  }
}
